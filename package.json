{
  "name": "@ticos/realtime-api",
  "version": "0.3.1",
  "description": "Official SDK for Ticos Realtime API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --noEmit && tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "docs:generate": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project scripts/tsconfig.json scripts/generate-api-docs.mts",
    "docs:serve": "serve docs",
    "documentation": "pnpm docs:generate && pnpm docs:serve",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build",
    "publish": "pnpm publish"
  },
  "author": "Tiwater <hi@tiwater.com>",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.24",
    "@types/websocket": "^1.0.10",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "audio-decode": "^2.2.2",
    "chai": "^5.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdoc-to-openapi": "^1.0.1",
    "mocha": "^11.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "serve": "^14.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "typescript-json-schema": "^0.65.1",
    "vitest": "^3.1.1",
    "websocket": "^1.0.35"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiwater/ticos-realtime-api.git"
  },
  "keywords": [
    "ticos",
    "realtime",
    "api",
    "websocket",
    "ai",
    "chat"
  ],
  "bugs": {
    "url": "https://github.com/tiwater/ticos-realtime-api/issues"
  },
  "homepage": "https://github.com/tiwater/ticos-realtime-api#readme"
}
